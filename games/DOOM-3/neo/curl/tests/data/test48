# Client-side
<client>
<server>
http
</server>
 <name>
HTTP with -d and -G and -I
 </name>
 <command>
-d "foo=moo&moo=poo" http://%HOSTIP:%HOSTPORT/48 -G -I
</command>
</client>

#
# Verify data after the test has been "shot"
<verify>
<strip>
^User-Agent:.*
</strip>
<protocol>
HEAD /48?foo=moo&moo=poo HTTP/1.1
Host: 127.0.0.1:8999
Pragma: no-cache
Accept: */*

</protocol>
</verify>
